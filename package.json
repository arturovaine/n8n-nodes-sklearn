{
  "name": "n8n-nodes-sklearn",
  "version": "0.6.0",
  "description": "n8n nodes for scikit-learn machine learning algorithms",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "scikit-learn",
    "sklearn",
    "machine-learning",
    "ml"
  ],
  "license": "MIT",
  "homepage": "https://github.com/arturovaine/n8n-nodes-sklearn",
  "author": {
    "name": "Arturo Vaine",
    "email": "arturo.vaine@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arturovaine/n8n-nodes-sklearn.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes/**/*.ts",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/SklearnLinearRegression/SklearnLinearRegression.node.js",
      "dist/nodes/SklearnLogisticRegression/SklearnLogisticRegression.node.js",
      "dist/nodes/SklearnDecisionTree/SklearnDecisionTree.node.js",
      "dist/nodes/SklearnRandomForest/SklearnRandomForest.node.js",
      "dist/nodes/SklearnGradientBoosting/SklearnGradientBoosting.node.js",
      "dist/nodes/SklearnSVM/SklearnSVM.node.js",
      "dist/nodes/SklearnKNN/SklearnKNN.node.js",
      "dist/nodes/SklearnNaiveBayes/SklearnNaiveBayes.node.js",
      "dist/nodes/SklearnKMeans/SklearnKMeans.node.js",
      "dist/nodes/SklearnStandardScaler/SklearnStandardScaler.node.js",
      "dist/nodes/SklearnMinMaxScaler/SklearnMinMaxScaler.node.js",
      "dist/nodes/SklearnLabelEncoder/SklearnLabelEncoder.node.js",
      "dist/nodes/SklearnOneHotEncoder/SklearnOneHotEncoder.node.js",
      "dist/nodes/SklearnPCA/SklearnPCA.node.js",
      "dist/nodes/SklearnTrainTestSplit/SklearnTrainTestSplit.node.js",
      "dist/nodes/SklearnMetrics/SklearnMetrics.node.js",
      "dist/nodes/SklearnDatasets/SklearnDatasets.node.js",
      "dist/nodes/SklearnCrossValidation/SklearnCrossValidation.node.js",
      "dist/nodes/SklearnGridSearchCV/SklearnGridSearchCV.node.js",
      "dist/nodes/SklearnDBSCAN/SklearnDBSCAN.node.js",
      "dist/nodes/SklearnAgglomerativeClustering/SklearnAgglomerativeClustering.node.js",
      "dist/nodes/SklearnIsolationForest/SklearnIsolationForest.node.js",
      "dist/nodes/SklearnSimpleImputer/SklearnSimpleImputer.node.js",
      "dist/nodes/SklearnPolynomialFeatures/SklearnPolynomialFeatures.node.js",
      "dist/nodes/SklearnRobustScaler/SklearnRobustScaler.node.js",
      "dist/nodes/SklearnTfidfVectorizer/SklearnTfidfVectorizer.node.js",
      "dist/nodes/SklearnFeatureSelection/SklearnFeatureSelection.node.js",
      "dist/nodes/SklearnSpectralClustering/SklearnSpectralClustering.node.js",
      "dist/nodes/SklearnMeanShift/SklearnMeanShift.node.js",
      "dist/nodes/SklearnTruncatedSVD/SklearnTruncatedSVD.node.js",
      "dist/nodes/SklearnNMF/SklearnNMF.node.js",
      "dist/nodes/SklearnNormalizer/SklearnNormalizer.node.js",
      "dist/nodes/SklearnBinarizer/SklearnBinarizer.node.js",
      "dist/nodes/SklearnVotingClassifier/SklearnVotingClassifier.node.js",
      "dist/nodes/SklearnStackingClassifier/SklearnStackingClassifier.node.js",
      "dist/nodes/SklearnCalibratedClassifierCV/SklearnCalibratedClassifierCV.node.js",
      "dist/nodes/SklearnElasticNet/SklearnElasticNet.node.js",
      "dist/nodes/SklearnRidgeLasso/SklearnRidgeLasso.node.js",
      "dist/nodes/SklearnMLP/SklearnMLP.node.js",
      "dist/nodes/SklearnPipeline/SklearnPipeline.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
